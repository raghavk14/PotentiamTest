@model UnitsReport.Models.UnitReportInputViewModel

@{
    ViewData["Title"] = "Home Page";
}

<div id="myCarousel" class="carousel slide" data-ride="carousel" data-interval="6000">
    <ol class="carousel-indicators">
        <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
        <li data-target="#myCarousel" data-slide-to="1"></li>
        <li data-target="#myCarousel" data-slide-to="2"></li>
    </ol>

    <form method="post" asp-controller="Home">
        <div class="carousel-inner" role="listbox">
            <div class="item active">
                <br />
                <div class="row">
                    <div>
                        @Html.Label("From Date", null, new { @class = "control-label col-xs-3" })
                        <div class="col-xs-3">
                            <input type="datetime-local" asp-for="FromDate" />
                        </div>
                    </div>
                </div>
                <br />
                <div class="row">
                    <div>
                        @Html.Label("To Date", null, new { @class = "control-label col-xs-3" })
                        <div class="col-xs-3">
                            <input type="datetime-local" asp-for="ToDate" />
                        </div>
                    </div>
                </div>
              
                    <input type="submit" value="Download" asp-action="Download" /> &nbsp;&nbsp;

                    <input type="submit" value="Display" asp-action="Display" />

                </div>
            </div>
    </form>
  
</div>

<div class="row">

    
</div>




